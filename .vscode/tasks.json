{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: clang build active file",
			"command": "/usr/bin/clang",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall", 
				"-Wextra", 
				"-Werror", 
				"-I",
				"incs",
				"srcs/*.c",
				"srcs/builtin/*.c",
				"-lreadline",
				"-L.",
				"-lft",
				"-o",
				"/Users/ski/Documents/2_Module_En_Cours/minishell/ski_ski"
				// "${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "/Users/ski/Documents/2_Module_En_Cours/minishell"
				// "cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/clang"
		}
	]
}